pipeline {
    agent any

    stages {
        stage ('Compile Stage') {
            steps {
                withGradle(gradle: 'gradle_6_1_1'){
                    sh './gradlew clean'
                }
            }
        }

        stage ('Testing Stage') {
            steps {
                withGradle(gradle: 'gradle_6_1_1') {
                    sh './gradlew build'
                }
            }
        }
    }
}